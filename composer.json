{
    "name": "meican2",
    "description": "MEICAN 2",
    "keywords": ["meican", "dynamic", "circuit", "network", "reservation", "topology", "nsi"],
    "homepage": "http://meican.cipo.rnp.br/",
    "type": "project",
    "license": "BSD-2-License",
    "support": {
        "wiki": "https://wiki.rnp.br"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.5.0",
        "yiisoft/yii2": "2.0.3",
        "yiisoft/yii2-bootstrap": "2.0.3",
        "yiisoft/yii2-swiftmailer": "2.0.3",
        "yiisoft/yii2-jui": "2.0.3",
        "thiagotalma/yii2-fullcalendar": "2.0.0"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "2.0.3",
        "yiisoft/yii2-debug": "2.0.3",
        "yiisoft/yii2-gii": "2.0.3",
        "yiisoft/yii2-faker": "2.0.3"
    },
    "config": {
        "process-timeout": 1800
    },
    "scripts": {
        "pre-update-cmd": [
            "php yii migrate"
        ],
        "post-install-cmd": [
            "yii\\composer\\Installer::postCreateProject",
            "php yii migrate"
        ]
    },
    "extra": {
        "yii\\composer\\Installer::postCreateProject": {
            "setPermission": [
                {
                    "runtime": "0777",
                    "web/assets": "0777",
                    "yii": "0755"
                }
            ],
            "generateCookieValidationKey": [
                "config/web.php"
            ]
        },
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
